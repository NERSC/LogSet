        :edison ;
    .
    
:cscratch1
    a logset:Subject ;
    dct:title "edison scratch1" ;
    dct:description "a lustre filesystem attached to edison" ;
    dcat:landingPage <http://www.nersc.gov/users/storage-and-file-systems/file-systems/> ;
