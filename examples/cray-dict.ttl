# Data dictionary relevant to logs found on a Cray XC
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# namespaces for base dictionaries:
#@base <https://portal.nersc.gov/project/m888/resilience/datasets/> .
@base <http://portal.nersc.gov/project/mpccc/sleak/resilience/datasets/>
@prefix logset: <logset#> .
@prefix ddict: <ddict#> .

# declare myself and set a prefix:
#@base <https://portal.nersc.gov/project/m888/resilience/datasets/> .
@base <http://portal.nersc.gov/project/mpccc/sleak/resilience/example/cray/>
@prefix : <cray-dict#> .

:
    a owl:Ontology ;
    dct:title "A data dictionary for Cray XC clusters and logs" ;
    rdfs:label "craydict" ;
    owl:imports logset: ;
    owl:imports ddict: ;
    owl:versionIRI <v0.2> ;
    .

# some Cray-specific Subject types:
:XC30
    a owl:Class ;
    rdfs:subClassOf ddict:Cluster ;
    rdfs:label "xc30" ;
    rdfs:comment "Cray XC30 Aries-based supercomputer"

:XC40
    a owl:Class ;
    rdfs:subClassOf ddict:Cluster ;
    rdfs:label "xc40" ;
    rdfs:comment "Cray XC40 Aries-based supercomputer"

:XE6
    a owl:Class ;
    rdfs:subClassOf ddict:Cluster ;
    rdfs:label "xe6" ;
    rdfs:comment "Cray XE6 Gemini-based supercomputer"


:Aries
    a owl:Class ;
    rdfs:subClassOf ddict:HSN ;
    rdfs:label "aries" ;
    rdfs:comment "Cray Aries high-speed network" ;
    .

:AriesRouter
    a owl:Class ;
    rdfs:subClassOf ddict:Switch ;
    rdfs:label "aries router" ;
    skos:altLabel "router" ;
    rdfs:comment "The high-radix tile-based router used by Cray Aries" ;
    .

:Gemini
    a owl:Class ;
    rdfs:subClassOf ddict:HSN ;
    rdfs:label "gemini" ;
    rdfs:comment "Cray Gemini high-speed network" ;
    .

:GeminiRouter
    a owl:Class ;
    rdfs:subClassOf ddict:Switch ;
    rdfs:label "gemini router" ;
    skos:altLabel "router" ;
    rdfs:comment "The high-radix tile-based router used by Cray Gemini" ;
    .

# This will be unused
:AriesRouterTile
    a owl:Class ;
    rdfs:subClassOf ddict:Port ;
    rdfs:label "aries router tile" ;
    skos:altLabel "tile" ;
    rdfs:comment "a single tile in an Aries router" ;
    .

:NetworkTile
    a owl:Class ;
    rdfs:subClassOf ddict:Port ;
    rdfs:label "aries router network facing tile" ;
    skos:altLabel "network tile" ;
    rdfs:comment "a single network facing tile in an Aries router" ;
    .

:PTile
    a owl:Class ;
    rdfs:subClassOf ddict:Port ;
    rdfs:label "aries router processor facing tile" ;
    skos:altLabel "ptile" ;
    rdfs:comment "a single processor facing tile in an Aries router" ;
    .

:BlueLink
    a owl:Class ;
    rdfs:subClassOf ddict:NetworkLink ;
    rdfs:label "aries blue link" ;
    rdfs:comment "aries optical (inter-group) link" ;
    .

:BlackLink
    a owl:Class ;
    rdfs:subClassOf ddict:NetworkLink ;
    rdfs:label "aries black link" ;
    rdfs:comment "aries copper cable (Y) link" ;
    .

:GreenLink
    a owl:Class ;
    rdfs:subClassOf ddict:NetworkLink ;
    rdfs:label "aries green link" ;
    rdfs:comment "aries chassis backplane (X) link" ;
    .

# maybe this should be something else
:PtileLink
    a owl:Class ;
    rdfs:subClassOf ddict:NetworkLink ;
#    rdfs:subClassOf ddict:Port ;
    rdfs:label "aries ptile" ;
    rdfs:comment """ aries tile providing link between the aries router and the
                     node NIC. Ptile is special is that it acts both as a network
                     link (between a node and an aries) and also as a port, on both
                     the node and aries side. So failure can propagate across it via
                     a <node> :hasPart <nic> ; <nic> :hasPart <ptile> but it also
                     delineates a hop via its classification as a NetworkLink
                 """ ;
    .

# Cray XC systems have a specific service node called the smw (system management
# workstation) node, which collects many logs
:SMW
    a owl:Class ;
    rdfs:subClassOf ddict:ServiceNode ;
    rdfs:label "smw" ;
    .

# Cray cluster nodes are often referred to by a specialized label called a cname:
:cname
    a owl:DatatypeProperty ;
    rdfs:comment """ Components of a Cray cluster are uniquely referred to via their cname, 
                     which incorporates the component's location in the cluster
                 """ ;
    rdfs:domain logset:Subject ;
    rdfs:range rdfs:Literal ;
    .


:aries_status_snapshots a skos:Concept .


:console_logfile
    a logset:LogSeries ;
    rdfs:comment """ a console logfile is timestamped-line-per-entry, generally
                     named like "console-20170131" and looks like:
  2017-09-06T15:20:21.827169-07:00 corismw1 craylog: message type: console, connected to port: 5150
  2017-09-06T15:20:21.827236-07:00 corismw1 craylog: testing connectivity
  2017-09-06T15:20:22.075706-07:00 corismw1 starting
  2017-09-06T15:22:42.809249-07:00 c2-0c0s0n1 Wait4Boot: Blade Controller Boot Protocol version - 1
  2017-09-06T15:22:42.809276-07:00 c2-0c0s0n1 Wait4Boot: EXIT ---> PXE Boot
                 """ ;
    logset:infoType ddict:log_messages ;
    logset:subject ddict:Cluster ;
    logset:logFormatType ddict:timeStampedLogFile ;
    logset:logFormatInfo "filepattern=console-<date:YYYYMMDD>$" ;
    logset:logFormatInfo "filepattern=console$" ;
    logset:logFormatInfo "ts_words=0" ;
    logset:logFormatInfo "part_word=1" ;
    .

:messages_logfile
    a logset:LogSeries ;
    rdfs:comment """ the messages_logfile collects /var/log/messages from all
                     of the nodes in the cluster, and looks like:
  <30>1 2017-09-06T15:26:39.977558-07:00 c0-0c0s0n1 systemd-modules-load 5894 p0-20170906t151820 -  Inserted module 'mlx4_en'
  <30>1 2017-09-06T15:26:39.977586-07:00 c0-0c0s0n1 systemd-modules-load 5894 p0-20170906t151820 -  Inserted module 'scsi_dh_alua'
  <30>1 2017-09-06T15:26:39.977591-07:00 c0-0c0s0n1 systemd-modules-load 5894 p0-20170906t151820 -  Inserted module 'scsi_dh_emc'
  <30>1 2017-09-06T15:26:39.977593-07:00 c0-0c0s0n1 systemd-modules-load 5894 p0-20170906t151820 -  Inserted module 'dm_multipath'
  <27>1 2017-09-06T15:26:39.977596-07:00 c0-0c0s0n1 systemd-sysctl 5920 p0-20170906t151820 -  Failed to open file '/boot/sysctl.conf-4.4.49-92.11.1_3.0-cray_ari_s', ignoring: No such file or directory
  <30>1 2017-09-06T15:26:39.977610-07:00 c0-0c0s0n1 systemd-sysusers 5936 p0-20170906t151820 -  Creating group adm with gid 477.
  <30>1 2017-09-06T15:26:39.977617-07:00 c0-0c0s0n1 systemd 1 p0-20170906t151820 -  Started Rebuild Dynamic Linker Cache.
                 """ ;
    logset:infoType ddict:log_messages ;
    logset:subject ddict:Cluster ;
    logset:logFormatType ddict:timeStampedLogFile ;
    logset:logFormatInfo "filepattern=messages-<date:YYYYMMDD>$" ;
    logset:logFormatInfo "filepattern=messages$" ;
    logset:logFormatInfo "ts_words=1" ;
    logset:logFormatInfo "part_word=2" ;
    .

:consumer_logfile
    a logset:LogSeries ;
    rdfs:comment """ the consumer_logfile collects messages from all nodes in
                     a multiline format that looks like:
  Wed Sep  6 15:23:43 2017 - rs_event_t at 0xc01070
  ev_id = 0x080040ed (ec_l1_failed)
  ev_src = ::c4-3
  ev_gen = ::c0-0c0s0n0
  ev_flag = 0x00000002 ev_priority = 0 ev_len = 144 ev_seqnum = 0x00000000
  ev_stp = 59b0756f.000eeaf9 [Wed Sep  6 15:23:43 2017]
  svcid 0: ::c4-3 = svid_inst=0x0/svid_type=0x0/svid_node=c4-3[rsn_node=0x0/rsn_type=0x6/rsn_state=0x6], err code 131124 - Cabinet Sensor Check Warning
  ev_data...
  00000000: 01 00 00 00 00 00 00 00 00 00 00 00 0c 06 00 00 *................*
  00000010: 10 30 00 00 00 00 00 00 01 00 00 00 34 00 02 00 *.0..........4...*
                 """ ;
    logset:infoType ddict:log_messages ;
    logset:subject ddict:Cluster ;
    logset:logFormatType ddict:unstructuredTextFile ; # actually it has structure, but too complex initially
    logset:logFormatInfo "filepattern=consumer-<date:YYYYMMDD>$" ;
    logset:logFormatInfo "filepattern=consumer$" ;
    .

:aries_lcb_status
    a logset:LogSeries ;
    logset:infoType :aries_status_snapshots ;
    logset:subject :Aries ;
    logset:logFormatType ddict:filePerTimepoint ;
    logset:logFormatInfo "filepattern=<label:>_aries_lcb_status.<time:YYYYMMDDHHmmss>.xz$" ;
    #logset:logFormatInfo "mediatype=application/x-xz"
    # will need a specific handler I think, eg:
    #logset:logFormatInfo "handler=AriesLcbStatusHandler"
    .

:bm_logfile
    a logset:LogSeries ;
    rdfs:comment """ looks like:
  <158>1 2015-02-17T08:03:54.188307-06:00 mutrino bootmanager 7133 - [hss_bm@34] Accepting a halt request
  <158>1 2015-02-17T08:04:41.744222-06:00 mutrino bootmanager 7133 - [hss_bm@34] Accepting a halt request
  <158>1 2015-02-17T08:17:44.820232-06:00 mutrino bootmanager 7133 - [hss_bm@34] Accepting a BOOT_BN_ONLY boot request
  <158>1 2015-02-17T08:17:44.820713-06:00 mutrino bootmanager 7133 - [hss_bm@34] Boot Sessionid: p0-20150217t080709
    """ ;
    logset:subject ddict:Cluster ;
    logset:logFormatType ddict:timeStampedLogFile ;

:some_logfile
    a logset:LogSeries ;
    rdfs:comment " placeholder " ;
    logset:subject ddict:Cluster ;
    .

