# Data dictionary relevant to logs found on a Cray XC
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

@prefix logset: <http://portal.nersc.gov/project/mpccc/sleak/resilience/datasets/logset#> .
@prefix ddict: <http://portal.nersc.gov/project/mpccc/sleak/resilience/datasets/ddict#> .

# declare myself and set a prefix:
#@base <https://raw.githubusercontent.com/NERSC/LogSet/master/examples> .
@base <file:///global/homes/s/sleak/Monitoring/Resilience/LogSet/examples/> .
@prefix : <cray-dict#> .

:
    a owl:Ontology ;
    dct:title "A data dictionary for Cray XC clusters and logs" ;
    rdfs:label "craydict" ;
    owl:imports logset: ;
    owl:imports ddict: ;
    owl:versionIRI <v0.2> ;
    .

# some Cray-specific Subject types:
:Aries
    a owl:Class ;
    rdfs:subClassOf ddict:HSN ;
    rdfs:label "aries" ;
    rdfs:comment "Cray Aries high-speed network" ;
    .

:AriesRouter
    a owl:Class ;
    rdfs:subClassOf ddict:Switch ;
    rdfs:label "aries router" ;
    skos:altLabel "router" ;
    rdfs:comment "The high-radix tile-based router used by Cray Aries" ;
    .

# This will be unused
:AriesRouterTile
    a owl:Class ;
    rdfs:subClassOf ddict:Port ;
    rdfs:label "aries router tile" ;
    skos:altLabel "tile" ;
    rdfs:comment "a single tile in an Aries router" ;
    .

:NetworkTile
    a owl:Class ;
    rdfs:subClassOf ddict:Port ;
    rdfs:label "aries router network facing tile" ;
    skos:altLabel "network tile" ;
    rdfs:comment "a single network facing tile in an Aries router" ;
    .

:PTile
    a owl:Class ;
    rdfs:subClassOf ddict:Port ;
    rdfs:label "aries router processor facing tile" ;
    skos:altLabel "ptile" ;
    rdfs:comment "a single processor facing tile in an Aries router" ;
    .

:BlueLink
    a owl:Class ;
    rdfs:subClassOf ddict:NetworkLink ;
    rdfs:label "aries blue link" ;
    rdfs:comment "aries optical (inter-group) link" ;
    .

:BlackLink
    a owl:Class ;
    rdfs:subClassOf ddict:NetworkLink ;
    rdfs:label "aries black link" ;
    rdfs:comment "aries copper cable (Y) link" ;
    .

:GreenLink
    a owl:Class ;
    rdfs:subClassOf ddict:NetworkLink ;
    rdfs:label "aries green link" ;
    rdfs:comment "aries chassis backplane (X) link" ;
    .

# maybe this should be something else
:PtileLink
    a owl:Class ;
    rdfs:subClassOf ddict:NetworkLink ;
#    rdfs:subClassOf ddict:Port ;
    rdfs:label "aries ptile" ;
    rdfs:comment """ aries tile providing link between the aries router and the
                     node NIC. Ptile is special is that it acts both as a network
                     link (between a node and an aries) and also as a port, on both
                     the node and aries side. So failure can propagate across it via
                     a <node> :hasPart <nic> ; <nic> :hasPart <ptile> but it also
                     delineates a hop via its classification as a NetworkLink
                 """ ;
    .

# Cray XC systems have a specific service node called the smw (system management
# workstation) node, which collects many logs
:SMW
    a owl:Class ;
    rdfs:subClassOf ddict:ServiceNode ;
    rdfs:label "smw" ;
    .

